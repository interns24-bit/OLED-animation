// Array of all bitmaps for convenience. (Total bytes used to store images in PROGMEM = 5280)
const int epd_bitmap_allArray_LEN = 10;
const unsigned char* epd_bitmap_allArray[10] = {
	epd_bitmap__a_frm0_100,
	epd_bitmap__a_frm1_100,
	epd_bitmap__a_frm2_100,
	epd_bitmap__a_frm3_100,
	epd_bitmap__a_frm4_100,
	epd_bitmap__a_frm5_100,
	epd_bitmap__a_frm6_100,
	epd_bitmap__a_frm7_100,
	epd_bitmap__a_frm8_100,
	epd_bitmap__a_frm9_100
};


void setup() {

 
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {  // Change i2c Value if required
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }

  
}

void loop() {
  
    // Display Animation
    for (int frame = 0; frame < epd_bitmap_allArray_LEN; frame++) {
         // epd_bitmap__a_frm0_100
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm0_100, 64, 64, 1);
  display.display();
  delay(frame_delay);

  // epd_bitmap__a_frm1_100
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm1_100, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm2_100
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm2_100, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm3_100
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm3_100, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm4_100
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm4_100, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm5_100
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm5_100, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm6_100
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm6_100, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm7_100
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm7_100, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm8_100
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm8_100, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm9_100
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm9_100, 64, 64, 1);
  display.display();
  delay(frame_delay);
    }

    // Optionally, you can adjust the frame delay here if needed
    if (frame_delay > 50) frame_delay -= 20;
}
