};

// Array of all bitmaps for convenience. (Total bytes used to store images in PROGMEM = 7920)
const int epd_bitmap_allArray_LEN = 15;
const unsigned char* epd_bitmap_allArray[15] = {
	epd_bitmap__a_frm0_400,
  epd_bitmap__a_frm1_80,
	epd_bitmap__a_frm2_80,
	epd_bitmap__a_frm3_80,
	epd_bitmap__a_frm4_80,
	epd_bitmap__a_frm5_80,
	epd_bitmap__a_frm6_80,
	epd_bitmap__a_frm7_80,
	epd_bitmap__a_frm8_80,
	epd_bitmap__a_frm9_80,
	epd_bitmap__a_frm10_80,
	epd_bitmap__a_frm11_80,
	epd_bitmap__a_frm12_80,
	epd_bitmap__a_frm13_80,
	epd_bitmap__a_frm14_320,
	
};


void setup() {

 
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {  // Change i2c Value if required
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }

  
}

void displayBlinkingText(const char* text, int blinkDuration, int blinkInterval) {
    unsigned long startTime = millis();
    bool isVisible = true;

    while (millis() - startTime < blinkDuration) {
        display.clearDisplay();
        
        if (isVisible) {
            display.setTextSize(1);
            display.setTextColor(WHITE);
            display.setCursor(40, 28); // Position the text at the center
            display.print(text);
        }
        
        display.display();
        isVisible = !isVisible; // Toggle visibility
        delay(blinkInterval); // Control the blink speed
    }
}

void loop() {
    // Display blinking text for 5 seconds with a blink interval of 500 ms
    displayBlinkingText("Wo ai ni", 7000, 500);

    // Display Animation
    for (int frame = 0; frame < epd_bitmap_allArray_LEN; frame++) {
  
  // epd_bitmap__a_frm0_400
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm0_400, 64, 64, 1);
  display.display();
  delay(frame_delay);

  // epd_bitmap__a_frm1_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm1_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm2_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm2_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm3_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm3_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm4_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm4_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm5_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm5_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm6_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm6_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm7_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm7_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm8_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm8_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm9_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm9_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

// epd_bitmap__a_frm10_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm10_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm11_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm11_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm12_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm12_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm13_80
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm13_80, 64, 64, 1);
  display.display();
  delay(frame_delay);

 // epd_bitmap__a_frm14_320
  display.clearDisplay();
  display.drawBitmap(32,0,epd_bitmap__a_frm14_320, 64, 64, 1);
  display.display();
  delay(frame_delay);
    }

    // Optionally, you can adjust the frame delay here if needed
    if (frame_delay > 50) frame_delay -= 20;
}
